package chap01;

import java.util.Scanner;

public class test {

	public static void main(String[] args) {

		Scanner stdIn = new Scanner(System.in);

        System.out.println("1부터 n까지의 합을 구합니다.");
         System.out.print("n값: ");
        int n = stdIn.nextInt();
		
		int i = 1;
		
		System.out.println((i + n) * 5);
		
		
		
	}

}
