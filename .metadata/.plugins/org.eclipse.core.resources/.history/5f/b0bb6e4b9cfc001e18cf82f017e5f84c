package chap01;

import java.util.Scanner;

public class test {

	public static void main(String[] args) {

		
		System.out.printf("%3d" + "|" + "%3d" + "1" + "%3d");
		
		
        for (int i = 1; i <= 9; i++) {
            for (int j = 1; j <= 9; j++)
                System.out.printf("%3d", i * j);
            System.out.println();
        }
	}

}
