package chap01;

import java.util.Scanner;

public class test {

	public static void main(String[] args) {

		Scanner stdIn = new Scanner(System.in);
		System.out.println("정사각형을 출력합니다.");
		System.out.printf("변의 길이: ");
		int a = stdIn.nextInt();
		
		for(int i = 1; i <= a; i++) {
			
				System.out.println("*".repeat(a));
			
			
		}
		
		
		
	}

}
